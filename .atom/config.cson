"*":
  core:
    disabledPackages: [
      "welcome"
      "go-to-line"
      "wrap-guide"
      "background-tips"
    ]
    packagesWithKeymapsDisabled: [
      "dash"
      "rails-transporter"
      "ruby-block"
      "git-plus"
      "gist-it"
      "atom-csscomb"
      "atom-beautify"
      "symbol-gen"
      "merge-conflicts"
      "zentabs"
      "git-diff"
      "open-on-bitbucket"
      "rails-model-schema"
      "linter"
      "symbols-tree-view"
      "move-panes"
      "atom-alignment"
      "sort-lines"
      "ruby-test"
    ]
    openEmptyEditorOnStart: false
    themes: [
      "dark-flat-ui"
      "outlander-syntax"
    ]
    destroyEmptyPanes: false
  editor:
    audioBeep: false
    invisibles: {}
    softWrap: true
    softWrapAtPreferredLineLength: true
    scrollPastEnd: true
    preferredLineLength: 1000
    tabType: "soft"
    showInvisibles: true
    fontFamily: "Ricty Diminished"
    fontSize: 12
  welcome:
    showOnStartup: false
  "atom-beautify":
    _analyticsUserId: "a4134265-e650-47ca-aa99-9dd91cba36a7"
    css_indent_comments: false
  "atom-color-highlight":
    scopes:
      css: "css"
      scss: "scss"
      less: "less"
      sass: "sass"
      stylus: "stylus"
    hideMarkersInComments: true
    hideMarkersInStrings: true
  "autocomplete-plus":
    autoActivationDelay: 0
    minimumWordLength: 1
    confirmCompletion: "tab"
    maxVisibleSuggestions: 15
  "exception-reporting":
    userId: "b9bd8307-6a1b-1da6-3e85-434e50b7cb26"
  "git-diff":
    showIconsInEditorGutter: true
  jslint:
    validateOnChange: true
    hideOnNoErrors: true
  "file-types":
    rb: "source.ruby.rails"
    erb: "text.html.ruby"
  "linter-scss-lint":
    executablePath: "/Users/seiyayoshitaka/.rbenv/shims/scss-lint"
  pigments:
    ignoredScopes: [
      "^(?!\\.source\\.[css|sass|scss|less|styl]).*"
    ]
    autocompleteScopes: [
      ".source.css"
      ".source.sass"
      ".source.scss"
      ".source.less"
      ".source.styl"
    ]
    markerType: "dot"
  "tabs-to-spaces":
    onSave: "untabify"
  "tree-view":
    hideIgnoredNames: true
  tabs:
    tabScrolling: true
    enableVcsColoring: true
  zentabs:
    neverCloseUnsaved: true
    showPinnedIcon: false
    maximumOpenedTabs: 8
  minimap:
    plugins:
      "minimap-autohide": true
      "git-diff": true
  "markdown-preview":
    useGitHubStyle: true
  linter:
    statusIconScope: "File"
  "clipboard-plus":
