      "~/dotfiles"
      "~/github"
      "~/Dropbox/jekyll"
      "~/Documents"
      "~/Vagrant"
    ]
    hideLoadedFolderFromAddList: false
  "find-and-replace": {}
  "linter-sass-lint":
