snippet dependent
options word
  dependent: ${1}

snippet destroy
options word
  destroy

snippet has_one
options word 
  has_one ${1}

snippet has_many
options word 
  has_many ${1}

snippet has_and_belongs_to_many
options word 
  has_and_belongs_to_many ${1}

snippet validates
options word 
  validates ${1}

snippet presence
options word
  presence: :${1}

snippet update_attribute
options word
  update_attribute(:${1})
