[core]
  excludesfile = /Users/seiyayoshitaka/.gitignore_global
  editor = vim -c \"set fenc=utf-8\"
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path = 
[mergetool "sourcetree"]
  cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[user]
  name = ystkme
  email = bloom6445@gmail.com
[filter "lfs"]
  clean = git lfs clean %f
  smudge = git lfs smudge %f
  required = true
[github]
  user = ystkme
[color]
  ui = auto
[alias]
  a	= add
  b  	= branch
  c  	= commit -v
  cm 	= commit
  co 	= checkout
  d  	= diff
  graph = log --graph --date=short --pretty='%C(yellow)%h%Creset %C(cyan bold)%d%Creset %s %Cgreen[%cd]%Creset' --decorate
  l	= graph
  p  	= push
  pl 	= !git pull && git submodule update --init
  s  	= status
[push]
  default = simple
[init]
  templatedir = ~/.git_tmp
